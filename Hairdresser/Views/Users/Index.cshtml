@model IEnumerable<IdentityUser>

	<a asp-action="personelEkle"> User Ekle</a>
<table class="table table-sm table-bordered">
	<thead>
		<tr>
			<th>Id</th>
			<th>Name</th>
			<th>Email</th>
		</tr>
	</thead>

	<tbody>
		@foreach(var user in Model)
		{
			<tr>
				<td>@user.Id</td>
				<td>@user.UserName</td>
				<td>@user.Email</td>
				<td><a href="">Düzenle</a></td>
			</tr>
		}
	</tbody>
</table>
